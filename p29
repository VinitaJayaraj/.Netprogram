using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter seconds:");
        int seconds = Convert.ToInt32(Console.ReadLine());

        int minutes = seconds / 60;
        int hours = minutes / 60;

        Console.WriteLine("Hours: " + hours + ", Minutes: " + (minutes % 60) + ", Seconds: " + (seconds % 60));
    }
}
