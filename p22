using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter a number:");
        int num = Convert.ToInt32(Console.ReadLine());

        if (num % 5 == 0 && num % 11 == 0)
            Console.WriteLine("Divisible by both 5 and 11");
        else
            Console.WriteLine("Not divisible by both 5 and 11");
    }
}
