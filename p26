using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter a character:");
        char ch = Convert.ToChar(Console.ReadLine());

        if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z'))
            Console.WriteLine(ch + " is an alphabet.");
        else
            Console.WriteLine(ch + " is not an alphabet.");
    }
}
