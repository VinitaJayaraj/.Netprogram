using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter the number of terms:");
        int terms = Convert.ToInt32(Console.ReadLine());

        int a = 0, b = 1, c;

        Console.Write(a + " " + b);

        for (int i = 2; i < terms; i++)
        {
            c = a + b;
            Console.Write(" " + c);
            a = b;
            b = c;
        }
    }
}
