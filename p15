using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter a string:");
        string str = Console.ReadLine();
        int count = 0;

        foreach (char c in str.ToLower())
        {
            if ("aeiou".Contains(c))
                count++;
        }

        Console.WriteLine("Number of vowels: " + count);
    }
}
