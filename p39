using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter two numbers:");
        int a = Convert.ToInt32(Console.ReadLine());
        int b = Convert.ToInt32(Console.ReadLine());

        a = a + b;
        b = a - b;
        a = a - b;

        Console.WriteLine("After Swapping: a = " + a + ", b = " + b);
    }
}
